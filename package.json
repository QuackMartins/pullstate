{
  "name": "pullstate",
  "version": "0.0.8",
  "description": "Simple state stores using immer and React hooks",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./dist",
    "build": "npm run clean && rollup -c",
    "build-full": "npm run clean && tsc",
    "uglify": "terser ./dist/index.js -o ./dist/index.js",
    "prepublish": "in-publish && npm run build-full",
    "benchmark-all": "ts-node test/benchmark/all.ts",
    "benchmark-async-argument": "ts-node test/benchmark/async-argument-benchmark.ts"
  },
  "keywords": [
    "immer",
    "state",
    "store",
    "react",
    "hooks"
  ],
  "author": "Paul Myburgh",
  "license": "MIT",
  "dependencies": {
    "fbjs": "^1.0.0",
    "immer": "^2.0.0"
  },
  "repository": "https://github.com/lostpebble/pullstate",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/jest": "23.3.10",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "benchmark": "^2.1.4",
    "in-publish": "^2.0.0",
    "jest": "23.6.0",
    "jest-environment-jsdom": "23.4.0",
    "jest-environment-jsdom-global": "1.1.0",
    "lodash": "^4.17.11",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.2",
    "rollup": "^1.2.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "terser": "^3.16.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "peerDependencies": {
    "react": "^16.8.2"
  },
  "jest": {
    "bail": false,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  }
}
